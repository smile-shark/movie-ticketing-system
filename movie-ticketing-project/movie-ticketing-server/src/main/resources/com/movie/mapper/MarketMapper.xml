<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.movie.mapper.MarketMapper">
    <select id="selectAllMarket" resultType="market">
        select * from market
    </select>
    <select id="selectMarketById" resultType="market">
        select * from market where market_id=#{marketId}
    </select>
    <select id="selectMarketByEconomizeId" resultType="market">
        select * from market where economize_id=#{economizeId}
    </select>
</mapper>
<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.movie.mapper.GlobalProfilePictureMapper">
    <select id="selectGlobalProfilePictureRandom" resultType="String">
        select global_profile_picture from global_profile_picture
        order by rand() limit 1
    </select>
</mapper>
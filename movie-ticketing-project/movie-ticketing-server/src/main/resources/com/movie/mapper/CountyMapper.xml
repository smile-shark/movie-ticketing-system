<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.movie.mapper.CountyMapper">
    <select id="selectCountyByMarketId" resultType="county">
        select * from county where market_id=#{marketId}
    </select>
    <select id="selectCountyByCountyId" resultType="county">
        select * from county where county_id=#{countyId}
    </select>
</mapper>